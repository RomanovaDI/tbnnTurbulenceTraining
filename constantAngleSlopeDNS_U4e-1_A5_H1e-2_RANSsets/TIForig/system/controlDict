stopAt          endTime;

endTime         endTimePattern;

//deltaT          5e-06;
deltaT          deltaTPattern;

//writeControl    adjustableRunTime;
writeControl    timeStep;

//writeInterval   1e-03;
writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable yes;

adjustTimeStep  no;//yes;

maxCo           1;
maxAlphaCo      1;

maxDeltaT       1;

